for i in range(1, 60):
    n = '1'*i
    while '111' in n:
        n = n.replace('111', '2', 1)
        n = n.replace('222', '11', 1)
        n = n.replace('1', '2', 1)
    if n.count('2') == len(n):
        print(i, n)
# 0, 9, 10 
print(123456794%16, 678901234%16)
print((678901234//16 - 123456794//16-1)*3+2)
